{
  "name": "dev-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage --karma-config=karma.conf.js",
    "coverage": "ng test --watch=false --code-coverage --karma-config=karma.conf.js && npx nyc report --reporter=text-lcov > coverage/lcov.info && npx codecov -f coverage/lcov.info",
    "analyze": "source-map-explorer dist/ms_dev_portal/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.13",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.13",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "chart.js": "^4.4.6",
    "dev-portal": "file:",
    "ng2-charts": "^6.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "codecov": "^3.8.3",
    "jasmine-core": "^5.4.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "nyc": "^17.1.0",
    "source-map-explorer": "^2.5.3",
    "symbol-observable": "^4.0.0",
    "typescript": "~5.5.2"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage/**",
      "dist/**",
      "node_modules/**",
      "src/**/*.spec.ts"
    ]
  }
}
